	function getUserLocale() {
		var userLoc = false;

		// Attempt to check for fastly cookie "location_data" and retrieve country code.
		try {
			cookie = JSON.parse(getCookieValue('location_data'), siteLocale);

			if (cookie !== null && cookie.hasOwnProperty('country_code')) {
				userLoc = cookie.country_code.toLowerCase();
			}

			return userLoc;
		} catch(e) {
			return false;
		}
	}

		window.GDPR_CONSENT_MODAL = function() {
				let oneTrustModalIsShowing = doc.querySelector('#onetrust-banner-sdk') ? true : false;
		return oneTrustModalIsShowing;
	};

	
				var gdprCookieChoicesButton = doc.querySelector('.ot-sdk-show-settings');
		var onetrust_pc_modal_event = new Event('onetrust_pc_modal');
		if (gdprCookieChoicesButton) {
			gdprCookieChoicesButton.addEventListener( 'click', function() {
				window.dispatchEvent(onetrust_pc_modal_event);
			}, false);
		}

						var onetrustCookieVal = doc.cookie.match("[;]*\s*OptanonConsent\s*=\s*([^;]+)");

				window.GDPR_CONSENT_GROUPS = "";
		if (onetrustCookieVal) {
			try {
				var groups = onetrustCookieVal[1].match(/groups=(.*?)(&|$)/);
				if (groups) {
					groups = decodeURIComponent(groups[1]).split(",");
					for (var i = 0; i < groups.length; i++) {
						if (groups[i].split(":")[1] === "1") {
							window.GDPR_CONSENT_GROUPS += ((window.GDPR_CONSENT_GROUPS ? "," : "") + groups[i].split(":")[0]);
						}
					}
				}
			} catch(e) {}
		}

				var haveConsent = (/C0002|C0003|C0004|C0005/.test(window.GDPR_CONSENT_GROUPS)) ? true : false;

				var onetrustAlertBoxCookieVal = doc.cookie.match("[;]*\s*OptanonAlertBoxClosed\s*=\s*([^;]+)");

				if (!haveConsent && onetrustAlertBoxCookieVal) window.GDPR_WITHDRAWN = true;

				var cookieTimestamp = 0;
		if (onetrustAlertBoxCookieVal) {
			var ds = new Date(onetrustAlertBoxCookieVal[1]);
			cookieTimestamp = ds.getTime();
		}

						function overwriteOTCookie() {
			if (!haveConsent && (cookieTimestamp - (Date.now() - (1000 * 60 * 60 * 12)) ) < 0) {
								var cookieString = "OptanonAlertBoxClosed=;expires=Thu, 01 Jan 1970 00:00:00 GMT;path=/;";
								if (env === 'prod') {
					var cookieDomain = window.location.hostname.replace("www","");
					cookieString += "domain=" + cookieDomain + ";";
				}
				doc.cookie = cookieString;
			}
		}

				if (gdprCountryOverrides.indexOf(siteLocale) === -1) {
			userLocale = getUserLocale();
			if (gdprCountryOverrides.indexOf(userLocale) === -1) {
				overwriteOTCookie();
			}
		}

				function checkOtTextScroll(sel) {
			setTimeout(function() {
				var otp = doc.querySelector(sel);
				if (otp && otp.scrollTop > 0) {
					otp.scrollTop = 0;
				}
			}, 250);
		}

				window.addEventListener('onetrust_modal', function() {
			checkOtTextScroll('#onetrust-policy');
		}, { once: true });

				window.addEventListener('onetrust_pc_modal', function() {
			checkOtTextScroll('#ot-pc-content');
		}, false);

				window.OptanonWrapper = function() {
						var haveCurrentConsent = /C0002|C0003|C0004|C0005/.test(OptanonActiveGroups);

						var onetrustAlertBoxCookieVal = doc.cookie.match("[;]*\s*OptanonAlertBoxClosed\s*=\s*([^;]+)");
			var otTimestamp = (onetrustAlertBoxCookieVal) ? new Date(onetrustAlertBoxCookieVal[1]).getTime() : '';
            var recentAction = (Date.now() - otTimestamp < 250);

						var modalShowing = window.GDPR_CONSENT_MODAL();
			if (modalShowing) {
				if (!window.ga_modal_tracking){
					var onetrust_modal_event = new Event("onetrust_modal");
					window.dispatchEvent(onetrust_modal_event);
				}

								var gdprLearnMoreButton = doc.querySelector('#onetrust-pc-btn-handler');
				if (gdprLearnMoreButton) {
					gdprLearnMoreButton.addEventListener( 'click', function() {
						window.dispatchEvent(onetrust_pc_modal_event);
					}, { once: true });
				}

								function showOTDeclineButton() {
					var otModal = doc.querySelector('#onetrust-banner-sdk');
					otModal.classList.add('with-decline-option');
				}
				if (gdprCountryOverrides.indexOf(siteLocale) > -1) {
					showOTDeclineButton();
				} else {
					userLocale = getUserLocale();
					if (gdprCountryOverrides.indexOf(userLocale) > -1) {
						showOTDeclineButton();
					}
				}
			}

						function waitForOTCookie() {
				function assignOTCookiePromise(resolve, reject) {
					if (doc.cookie.indexOf('OptanonAlertBoxClosed') > -1) {
						resolve(true);
					} else {
						setTimeout(assignOTCookiePromise.bind(this, resolve, reject), 100);
					}
				}
				return new Promise(assignOTCookiePromise);
			}

						if (modalShowing && recentAction) {
								if (haveCurrentConsent) {
					var onetrust_accept_event = new Event("onetrust_accept");
					window.dispatchEvent(onetrust_accept_event);
				} else {
					var onetrust_reject_event = new Event("onetrust_reject");
					window.dispatchEvent(onetrust_reject_event);
				}

				waitForOTCookie().then(() => location.reload());
			}
		};

				(function loadOneTrust() {
			var domainId = '1f75471b-fffc-4fa4-bcec-b5c543a1c09a' + ((env === 'stage') ? '-test' : '');
			var s = doc.createElement('script');
			s.type = 'text/javascript';
			s.async = true;
			s.charset = 'UTF-8';
			s.src = 'https://cdn.cookielaw.org/scripttemplates/otSDKStub.js';
			s.setAttribute('data-domain-script', domainId);
			s.setAttribute('data-document-language', 'true');
			bod.appendChild(s);
		})();

				if (haveConsent) {
			s.src = "//nexus.ensighten.com/hearst/mag-gdpr/Bootstrap.js";
									window.GDPR_CONSENT = true;

		} else {
			s.src = "//nexus.ensighten.com/hearst/mag-dnt/Bootstrap.js";
					}

	
	if ( doc.cookie.indexOf( "nsghtn=dev" ) > -1 ) {
		s.src = "//nexus.ensighten.com/hearst/mag-dev/Bootstrap.js";
	}

						head.appendChild( s );
			
			
						})(document);
</script>
		</body>
	</html>

