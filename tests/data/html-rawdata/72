	        var price = Math.round(parseFloat(product.priceNoSymbol) || 0)
			productPrice = product.price.replace(product.priceNoSymbol, price)
			if (!price) {
				productPrice = "Shop"
			}
		} else {
			productPrice = "Shop"
		}

